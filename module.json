{
  "id": "project-greyhaven",
  "title": "An Example FVTT Module",
  "description": "A simple module created as a demo for working in the Foundry Virtual Tabletop framework.",
  "authors": [
    {
      "name": "the.great.old.one.sleeps"
    }
  ],
  "version": "0.0.0",
  "compatibility": {
    "minimum": "11",
    "verified": "13"
  },
  "scripts": [
    "scripts/hello.js"
  ],
  "packs": [
    {
      "name": "gh-npcs",
      "label": "Greyhaven NPCs",
      "system": "dnd5e",
      "path": "packs/npcs",
      "type": "Actor",
      "ownership": {
        "PLAYER": "NONE",
        "TRUSTED": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "gh-journals",
      "label": "Greyhaven Journals",
      "system": "dnd5e",
      "path": "packs/journals",
      "type": "JournalEntry",
      "ownership": {
        "PLAYER": "NONE",
        "TRUSTED": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "gh-factions",
      "label": "Greyhaven Factions",
      "system": "dnd5e",
      "path": "packs/factions",
      "type": "JournalEntry",
      "ownership": {
        "PLAYER": "NONE",
        "TRUSTED": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "gh-blight",
      "label": "Greyhaven Blight",
      "system": "dnd5e",
      "path": "packs/blight",
      "type": "JournalEntry",
      "ownership": {
        "PLAYER": "NONE",
        "TRUSTED": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "gh-tables",
      "label": "Greyhaven Roll Tables",
      "system": "dnd5e",
      "path": "packs/tables",
      "type": "RollTable",
      "ownership": {
        "PLAYER": "NONE",
        "TRUSTED": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    }
  ]
